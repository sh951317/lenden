#!/bin/bash


CURRENT_VERSION='1.0.1'
Ver_Sion="echo ${CURRENT_VERSION} | awk -F "." '{print $1,$2,$3}'"
echo ${Ver_Sion}
versionParts=CURRENT_VERSION.split('\\.')
major = versionParts[0].toInteger()
minor = versionParts[1].toInteger()
patch = versionParts[2].toInteger()	
CURRENT_VERSION2 =  "oc get is/${APP_NAME} -n ${NAME_SPACE} --output=jsonpath='{.status.tags[*].tag}'"
patch = (patch.toInteger() + 1).toString()
NEW_VERSION = "${major}.${minor}.${patch}"
echo "Current version is: ${CURRENT_VERSION}"
echo "New version is: ${NEW_VERSION}"
echo "TCS Current version is: ${CURRENT_VERSION_Later}"
